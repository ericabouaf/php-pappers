{
    "name": "ericabouaf/pappers",
    "description": "PHP library to access Pappers.fr API",
    "keywords": ["pappers"],
    "type": "library",
    "homepage": "https://github.com/ericabouaf/php-pappers",
    "config": {
        "platform": {
            "php": ">=7.4"
        }
    },
    "require": {
        "symfony/http-client": "^4.3||^5.0",
        "symfony/serializer": "^4.3||^5.0",
        "symfony/property-info": "^4.3",
        "symfony/property-access": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.77"
    },
    "autoload": {
        "psr-4": {
            "Neyric\\Pappers\\": "src",
            "Neyric\\Pappers\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyze src --level 8"
    },
    "authors": [
        {
            "name": "Eric Abouaf",
            "email": "eric.abouaf@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
